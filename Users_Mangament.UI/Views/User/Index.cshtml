@using Users_Mangament.Domain.Models
@model IEnumerable<User>
<hr />
<hr />
<h2>Users Index</h2>
<a asp-action="Create" >Create</a> |
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>FullName</th>
           
            <th>Email</th>
            <th>PhoneNumber</th>
            <th>BirthDate</th>
            <th>IsActive</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.FullName</td>
               
                <td>@user.Email</td>
                <td>@user.PhoneNumber</td>
                <td>@user.BirthDate</td>
                <td>@user.IsActive</td>
                <td>
                    <a asp-action="Details" asp-route-id="@user.Id">Details</a> |
                    <a asp-action="Edit" asp-route-id="@user.Id">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@user.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
    <script src="~/js/filesize-validator.js" asp-append-version="true"></script>
    <script src="~/js/game-form.js" asp-append-version="true"></script>
}